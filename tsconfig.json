{
  "compilerOptions": {
    "target": "es2022",
    "module": "node16",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
